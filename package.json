    "json-loader": "^0.5.4",
    "ramda": "^0.21.0",
    "react": "0.14.8",
    "react-dom": "^0.14.7",
    "react-markdown": "^2.2.0",
    "snyk": "^1.316.1"
  },
  "repository": {
    "type": "git",
